<Window x:Class="Stock.Delete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stock"
        mc:Ignorable="d"
        Title="Delete" Height="400" Width="400" ResizeMode="NoResize">
    <Grid>
		<ComboBox x:Name="DeleteList"  Margin="15,35,0,0"  SelectionChanged="DeleteList_Selected" HorizontalAlignment="Left" VerticalAlignment="Top" Width="249"/>
		<Button x:Name="Delet" Content="Удалить" Click="Delet_Click" HorizontalAlignment="Left" Margin="282,35,0,0" VerticalAlignment="Top" Height="37" Width="93"/>
		<DataGrid x:Name="DeleteTable" HorizontalAlignment="Center"   Width="370" Margin="0,91,0,10" AutoGenerateColumns="True"/>

	</Grid>
</Window>
